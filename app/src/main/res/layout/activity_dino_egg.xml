<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2E2E2A"
    tools:context=".Shop">

    <TextView
        android:id="@+id/TextView_shop_dino_egg"
        android:layout_width="122dp"
        android:layout_height="24dp"
        android:layout_marginEnd="4dp"
        android:layout_marginRight="4dp"
        android:background="@drawable/stroke"
        android:gravity="center"
        android:text="DinoBoneCount"
        android:textColor="#FFFFFF"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022"
        android:fontFamily="@font/goyangdeogyangb"/>

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="32dp"
        android:layout_height="29dp"
        android:layout_marginStart="252dp"
        android:layout_marginLeft="252dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.018"
        app:srcCompat="@drawable/dinohead" />


    <Button
        android:id="@+id/button11"
        android:layout_width="match_parent"
        android:layout_height="86dp"
        android:layout_marginHorizontal="30dp"
        android:background="@drawable/advertisingbutton"
        android:fontFamily="@font/goyangdeogyangb"
        android:shadowColor="#000000"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="5"
        android:text="Get dinobone \n by watching ads"
        android:textColor="#FFFFFF"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/TextView_dinobone1000"/>

    <TextView
        android:id="@+id/TextView_dinobone200"
        android:layout_width="87dp"
        android:layout_height="46dp"
        android:layout_marginTop="12dp"
        android:gravity="center"
        android:text="1 $"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.104"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Button_dinobone200"
        app:layout_constraintVertical_bias="0.0"
        android:fontFamily="@font/goyangdeogyangb"/>

    <TextView
        android:id="@+id/TextView_dinobone1000"
        android:layout_width="87dp"
        android:layout_height="46dp"
        android:layout_marginTop="12dp"
        android:gravity="center"
        android:text="5 $"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Button_dinobone200"
        app:layout_constraintVertical_bias="0.0"
        android:fontFamily="@font/goyangdeogyangb"/>

    <TextView
        android:id="@+id/TextView_dinobone2000"
        android:layout_width="87dp"
        android:layout_height="46dp"
        android:layout_marginTop="12dp"
        android:gravity="center"
        android:text="10 $"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.895"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Button_dinobone200"
        app:layout_constraintVertical_bias="0.0"
        android:fontFamily="@font/goyangdeogyangb"/>

    <Button
        android:id="@+id/Button_dinobone200"
        android:layout_width="100dp"
        android:layout_height="120dp"
        android:adjustViewBounds="true"
        android:scaleType="centerCrop"
        android:background="@drawable/dinohead_button_click"
        android:centerColor="#FFFFFF"
        android:endColor="#FFFFFF"
        android:gravity="center"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="5"
        android:startColor="#FFFFFF"
        android:text="X 200"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.056"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.334" />

    <Button
        android:id="@+id/Button_dinobone1000"
        android:layout_width="100dp"
        android:layout_height="120dp"
        android:background="@drawable/dinohead_button_click"
        android:centerColor="#FFFFFF"
        android:endColor="#FFFFFF"
        android:gravity="center"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="5"
        android:startColor="#FFFFFF"
        android:text="X 1000"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.334" />

    <Button
        android:id="@+id/Button_dinobone2000"
        android:layout_width="100dp"
        android:layout_height="120dp"
        android:background="@drawable/dinohead_button_click"
        android:centerColor="#FFFFFF"
        android:endColor="#FFFFFF"
        android:gravity="center"
        android:shadowDx="0"
        android:shadowDy="0"
        android:shadowRadius="5"
        android:startColor="#FFFFFF"
        android:text="X 2000"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.944"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.334" />

    <Button
        android:id="@+id/button20"
        android:layout_width="60dp"
        android:layout_height="63dp"
        android:background="@drawable/ark_quiz_main"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.014"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.007" />



</androidx.constraintlayout.widget.ConstraintLayout>